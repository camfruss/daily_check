<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.cornell.dailycheck</string>
        <key>KeepAlive</key>
        <false/>
        <key>RunAtLoad</key>
        <false/>
        <key>ProgramArguments</key>
        <array>
            <string>~/PycharmProjects/daily_check/venv/lib/python3</string>
            <string>~/PycharmProjects/daily_check/daily_check.py</string>
            <string>NETID</string>
            <string>PASSWORD</string>
        </array>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Hour</key>
            <integer>16</integer>
            <key>Minute</key>
            <integer>00</integer>
        </dict>
        <key>StandardOutPath</key>
        <string>~/PycharmProjects/daily_check/logging/out.log</string>
        <key>StandardErrorPath</key>
        <string>~/PycharmProjects/daily_check/logging/error.log</string>
    </dict>
</plist>
